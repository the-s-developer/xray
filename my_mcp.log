from urllib.parse import urljoin
import time

BASE_URL = "https://www.meb.gov.tr"
LIST_URL = "https://www.meb.gov.tr/meb_duyuruindex.php"
MAX_COUNT = 50  # toplamak istediğiniz kayıt limiti

RESULT = []

def get_announcements_on_page(page):
    result = []
    # Duyuru listesi tablo satırlarını bul
    rows = page.query_selector_all('table tr')
    for row in rows[1:]:  # başlık satırını atla
        tds = row.query_selector_all('td')
        if len(tds) < 2:
            continue
        tarih = tds[0].inner_text().strip()
        link_elem = tds[1].query_selector('a')
        if not link_elem:
            continue
        baslik = link_elem.inner_text().strip()
        detay_url = urljoin(BASE_URL, link_elem.get_attribute('href'))
        result.append({
            'tarih': tarih,
            'baslik': baslik,
            'detay_url': detay_url
        })
    # Pagination: Sonraki butonunu bul
    next_url = None
    next_btn = page.query_selector('a:has-text(\"Sonraki\")')
    if next_btn and next_btn.is_enabled():
        next_url = urljoin(BASE_URL, next_btn.get_attribute('href'))
    return result, next_url

def get_detail_info(page):
    title = page.query_selector('h2')
    title = title.inner_text().strip() if title else None
    pub_date = None
    for p in page.query_selector_all('p'):
        txt = p.inner_text()
        if 'Yayın Tarihi:' in txt:
            pub_date = txt.replace('Yayın Tarihi:', '').strip()
            break
    # Açıklama metni/paragrafları
    content = []
    for p in page.query_selector_all('p'):
        txt = p.inner_text().strip()
        if txt and 'Yayın Tarihi:' not in txt and 'Hakları Saklıdır' not in txt and 'T.C.' not in txt:
            content.append(txt)
        if len(content) >= 10:
            break
    # Ek dosyalar
    attachments = []
    for a in page.query_selector_all('a'):
        href = a.get_attribute('href')
        if href and any(href.lower().endswith(ext) for ext in ['.pdf', '.xls', '.xlsx', '.doc', '.docx']):
            name = a.inner_text().strip()
            abs_url = urljoin(page.url, href)
            attachments.append({'name': name, 'url': abs_url, 'ext': href.split('.')[-1]})
    return {
        'title': title,
        'publication_date': pub_date,
        'main_text': content,
        'attachments': attachments
    }

with sync_playwright() as p:
    browser = p.chromium.launch(headless=True)
    page = browser.new_page()
    page.goto(LIST_URL)
    all_announcements = []
    collected = 0
    while True:
        items, next_url = get_announcements_on_page(page)
        all_announcements.extend(items)
        collected += len(items)
        if not next_url or (MAX_COUNT and collected >= MAX_COUNT):
            break
        page.goto(next_url)
        # print(f"Gidilen sayfa: {next_url}")
        time.sleep(1)

    # Detay verilerini çek
    for i, item in enumerate(all_announcements):
        if MAX_COUNT and i >= MAX_COUNT:
            break
        try:
            page.goto(item['detay_url'])
            detail = get_detail_info(page)
            item.update(detail)
        except Exception as e:
            item['detail_error'] = str(e)
        RESULT.append(item)
        time.sleep(0.5)
    browser.close()


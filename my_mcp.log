from bs4 import BeautifulSoup
from urllib.parse import urljoin

# Scraping a single detail page for demo (MAX_COUNT not used here)
URL = "https://personel.meb.gov.tr/www/mill-egitim-uzman-yardimciligi-yarisma-sinavinda-basarili-olarak-atanmaya-hak-kazananlara-iliskin-duyuru/icerik/1628"
BASE_URL = "https://personel.meb.gov.tr"

RESULT = []

response = requests.get(URL)
soup = BeautifulSoup(response.content, "html.parser")

# Title
title = soup.find("h2").get_text(strip=True) if soup.find("h2") else None

# Publication date
pub_date = None
for p in soup.find_all('p'):
    txt = p.get_text()
    if 'Yayın Tarihi:' in txt:
        pub_date = txt.replace('Yayın Tarihi:', '').strip()
        break

# Main body text (first 10 paragraphs for demo)
content_blocks = []
for p in soup.find_all('p'):
    text = p.get_text(" ", strip=True)
    if text and 'Yayın Tarihi:' not in text and 'Hakları Saklıdır' not in text and 'T.C.' not in text:
        content_blocks.append(text)
    if len(content_blocks) >= 10:
        break

# Attachments (pdf, doc, xls links)
attachments = []
for a in soup.find_all('a', href=True):
    href = a['href']
    if any(href.lower().endswith(ext) for ext in ['.pdf','.xls','.xlsx','.doc','.docx']):
        name = a.get_text(strip=True)
        abs_link = urljoin(BASE_URL, href)
        attachments.append({'name': name, 'url': abs_link, 'ext': href.split('.')[-1]})

RESULT.append({
    'title': title,
    'publication_date': pub_date,
    'main_text': content_blocks,
    'attachments': attachments
})


from bs4 import BeautifulSoup
from urllib.parse import urljoin
import urllib3

urllib3.disable_warnings(urllib3.exceptions.InsecureRequestWarning)
BASE_URL = "https://www.meb.gov.tr"
LIST_URL = "https://www.meb.gov.tr/meb_duyuruindex.php"

RESULT = []
count = 0
MAX_COUNT = 5000  # Gerekirse dışarıdan set edilebilir

def scrape_list_page(list_url):
    global count
    resp = requests.get(list_url, verify=False)
    soup = BeautifulSoup(resp.content, "html.parser")
    # Duyuru satırları: tablo içinde
    table = soup.find("table")
    if not table:
        return []
    rows = table.find_all("tr")[1:-1]  # Başlık ve footer hariç
    data = []
    for row in rows:
        cols = row.find_all("td")
        if len(cols) < 2:
            continue
        tarih = cols[0].get_text(strip=True)
        link_elem = cols[1].find("a")
        if link_elem and link_elem.has_attr("href"):
            full_url = urljoin(BASE_URL, link_elem["href"])
            baslik = link_elem.get_text(strip=True)
        else:
            full_url = None
            baslik = cols[1].get_text(strip=True)
        data.append({
            "tarih": tarih,
            "baslik": baslik,
            "detay_url": full_url
        })
        count += 1
        if count >= MAX_COUNT:
            break
    # Sayfalama için bir sonraki sayfa linkini bul
    next_page = None
    pagination = soup.find("nav") or soup.find("div", class_="pagination")
    if pagination:
        links = pagination.find_all("a")
        for l in links:
            if "Sonraki" in l.text and l.has_attr("href"):
                next_page = urljoin(BASE_URL, l["href"])
                break
    else:
        # alternatif: nav class veya sayfa numaralarından
        nav = soup.find("nav")
        if nav:
            for l in nav.find_all("a"):
                if "Sonraki" in l.text and l.has_attr("href"):
                    next_page = urljoin(BASE_URL, l["href"])
                    break
    return data, next_page

current_url = LIST_URL
while current_url and count < MAX_COUNT:
    data, next_url = scrape_list_page(current_url)
    RESULT.extend(data)
    if not next_url or count >= MAX_COUNT:

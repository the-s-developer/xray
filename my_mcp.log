from playwright.sync_api import sync_playwright

RESULT = []  # Initialize RESULT as an empty list (will be returned as JSON data)

BASE_URL = "https://www.meb.gov.tr"

def get_announcement_content(page, url):
    page.goto(url)
    # Extract the title
    title = page.locator("h2").first.inner_text()
    # Extract the full content (concatenate all announcement text paragraphs)
    paragraphs = page.locator("div:has(h2) p").all_inner_texts()
    if not paragraphs:
        # fallback: try all <p> on page
        paragraphs = page.locator("p").all_inner_texts()
    content = "\n".join(paragraphs).strip()
    # Extract date (may be on page as "Yayın Tarihi: dd.mm.yyyy", fallback to meta or first date found)
    date = ""
    for p in paragraphs:
        if "Yayın Tarihi:" in p:
            date = p.split("Yayın Tarihi:")[-1].strip()
            break
    if not date:
        # fallback: try to find date near h2 (in e.g. small/strong/span)
        text_block = page.locator("div:has(h2)").inner_text()
        import re
        dates = re.findall(r"\d{2}[./]\d{2}[./]\d{4}", text_block)
        if dates:
            date = dates[0]

    return title, content, date


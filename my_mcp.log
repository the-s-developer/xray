RESULT = []  # Initialize RESULT as an empty list (will be returned as JSON data)

from urllib.parse import urljoin
from playwright.sync_api import sync_playwright

MAIN_URL = 'https://meb.gov.tr/meb_duyuruindex.php'

with sync_playwright() as p:
    browser = p.chromium.launch(headless=True)
    page = browser.new_page()
    page.goto(MAIN_URL)
    # Wait for table to load
    page.wait_for_selector('table')
    # Parse the table rows (skip header)
    rows = page.query_selector_all('table tr')[1:]
    for i, row in enumerate(rows):
        if i >= MAX_COUNT:
            break
        try:
            tds = row.query_selector_all('td')
            date = tds[0].inner_text().strip()
            title_elem = tds[1].query_selector('a')
            title = title_elem.inner_text().strip()
            url = title_elem.get_attribute('href')
            url = urljoin(MAIN_URL, url)
            # Visit detail page for content
            detail_page = browser.new_page()
            detail_page.goto(url)
            try:
                heading = detail_page.query_selector('h2')
                content = ''
                for el in detail_page.query_selector_all('p'):
                    c = el.inner_text().strip()
                    if c:
                        content += c + '\n'
            except Exception:
                content = ''
            detail_page.close()
            RESULT.append({
                'date': date,
                'title': title,
                'url': url,
                'content': content.strip()
            })
        except Exception:
            continue


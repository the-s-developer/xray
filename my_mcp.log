            detail_url = BASE_URL + detail_url
        elif detail_url.startswith("http"):
            pass  # keep as is
        else:
            detail_url = BASE_URL + "/" + detail_url.lstrip("/")

        # Go to detail page and extract info
        detail_title, detail_content, detail_date = get_announcement_content(page, detail_url)

        RESULT.append({

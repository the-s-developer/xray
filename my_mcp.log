import httpx

# URL of the announcements list page
base_url = "https://meb.gov.tr"
list_url = f"{base_url}/meb_duyuruindex.php"

# MAX_COUNT to limit number of announcements to scrape
MAX_COUNT = 5

# Function to get absolute URLs

def get_absolute_url(url):
    if url.startswith("http"):
        return url
    if url.startswith("/"):
        return base_url + url
    return base_url + "/" + url

# To store results
RESULT = []

# Get the announcements list page
client = httpx.Client()
response = client.get(list_url)
soup = BeautifulSoup(response.text, "html.parser")

# Find announcement detail links
announcement_links = []
for link in soup.select("table tr td:nth-child(2) a"):
    href = link.get("href")
    if href:
        full_url = get_absolute_url(href)
        announcement_links.append(full_url)

# Limit by MAX_COUNT
announcement_links = announcement_links[:MAX_COUNT]

# Scrape each announcement detail
for url in announcement_links:
    resp = client.get(url)
    detail_soup = BeautifulSoup(resp.text, "html.parser")

    title_elem = detail_soup.select_one("h2")
    title = title_elem.text.strip() if title_elem else ""
    
    # Publish date is often found in a paragraph near the content
    publish_date = ""
    for p in detail_soup.find_all("p"):
        if "Yayın Tarihi:" in p.text:
            publish_date = p.text.replace("Yayın Tarihi:","").strip()
            break

    # Extract all text paragraphs in generic container
    content = "".join(p.text.strip() + "\n" for p in detail_soup.select("div.generic p"))

    RESULT.append({
        "url": url,
        "title": title,
        "publish_date": publish_date,
        "content": content.strip()
    })

RESULT


import requests
from bs4 import BeautifulSoup
from urllib.parse import urljoin

BASE_URL = 'https://meb.gov.tr/meb_duyuruindex.php'
DETAIL_BASE = 'https://meb.gov.tr'

session = requests.Session()
response = session.get(BASE_URL)
soup = BeautifulSoup(response.content, 'html.parser')

# Find the table (announcements) by searching for date-format cells
announcements = []
rows = []
for table in soup.find_all('table'):
    trs = table.find_all('tr')
    if len(trs) < 2:
        continue
    tds = trs[1].find_all('td')
    if tds and any('/' in td.get_text() for td in tds):
        rows = trs[1:]
        break

for i, row in enumerate(rows):
    if i >= MAX_COUNT:
        break
    cols = row.find_all('td')
    if len(cols) < 2:
        continue
    date = cols[0].get_text(strip=True)
    link_tag = cols[1].find('a')
    if not link_tag:
        continue
    title = link_tag.get_text(strip=True)
    detail_href = link_tag['href']
    url = detail_href if detail_href.startswith('http') else urljoin(DETAIL_BASE, detail_href)
    # Try catch detail request
    try:
        detail_resp = session.get(url, timeout=10)
        detail_soup = BeautifulSoup(detail_resp.content, 'html.parser')
        # Try to extract main content: after h1 or h2
        heading = detail_soup.find(['h1', 'h2'])
        content_blocks = []
        if heading:
            for sibling in heading.find_all_next():
                if sibling.name in ['h1', 'h2']:
                    break
                if sibling.name in ['p', 'div', 'ul', 'ol', 'li']:
                    text = sibling.get_text(strip=True, separator=' ')
                    if text:
                        content_blocks.append(text)
        content = '\n'.join(content_blocks)
        # Fallback: meta description
        if not content:
            desc = detail_soup.find('meta', attrs={'name':'description'})
            if desc:
                content = desc.get('content','').strip()
    except Exception as e:
        content = ''
    announcements.append({
        'date': date,
        'title': title,
        'url': url,
        'content': content
    })

RESULT = announcements

